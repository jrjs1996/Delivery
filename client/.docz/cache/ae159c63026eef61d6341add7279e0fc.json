,CAACwB,IAHM;AAIvBd,EAAAA,UAAU,EAAEV,SAAS,CAACyB,MAJC;AAKvBd,EAAAA,QAAQ,EAAEX,SAAS,CAAC0B,IAAV,CAAeC,UALF;AAMvBf,EAAAA,KAAK,EAAEZ,SAAS,CAACyB,MANM;AAOvBZ,EAAAA,KAAK,EAAEb,SAAS,CAACuB,MAPM;AAQvBT,EAAAA,WAAW,EAAEd,SAAS,CAAC0B;AARA,CAAzB;AAWApB,YAAY,CAACsB,YAAb,GAA4B;AAC1BrB,EAAAA,WAAW,EAAE,EADa;AAE1BC,EAAAA,EAAE,EAAE,IAFsB;AAG1BC,EAAAA,KAAK,EAAE,KAHmB;AAI1BC,EAAAA,UAAU,EAAE,IAJc;AAK1BE,EAAAA,KAAK,EAAE,IALmB;AAM1BC,EAAAA,KAAK,EAAE,IANmB;AAO1BC,EAAAA,WAAW,EAAE;AAPa,CAA5B;;;;;;;;;;0BAzDwBR,Y","sourcesContent":["/* eslint-disable no-nested-ternary */\nimport React from 'react';\nimport PropTypes from 'prop-types';\nimport { Button, Paper } from '@material-ui/core';\nimport SettingPage from '../../Settings/SettingPage/SettingPage';\nimport SettingPageInput from '../../Settings/SettingPage/SettingPageInput/SettingPageInput';\nimport ImageUpload from './ImageUpload/ImageUpload';\n\n\nexport default function MenuItemForm({\n  description,\n  id,\n  image,\n  menuNumber,\n  onSubmit,\n  price,\n  title,\n  uploadImage,\n}) {\n  return (\n    <div className=\"MenuItemForm\">\n      <SettingPage\n        title=\"Item\"\n        onSubmit={(data) => {\n          if (id) {\n            data._id = id;\n          }\n          return onSubmit(data);\n        }}\n      >\n        <SettingPageInput required fullWidth name=\"title\" label=\"Title\" value={title} />\n        <SettingPageInput required fullWidth name=\"price\" label=\"Price\" value={price} />\n        <SettingPageInput required fullWidth name=\"menuNumber\" label=\"Number\" value={menuNumber} />\n        <SettingPageInput required fullWidth name=\"description\" multiline label=\"Description\" value={description} />\n      </SettingPage>\n      <Paper>\n        { image ? (\n          <div>\n            <img src={`/uploads/${image}`} alt=\"Item\" style={{ height: '100%', width: '100%', objectFit: 'contain' }} />\n            <br />\n            <Button\n              color=\"secondary\"\n              variant=\"contained\"\n              style={{ marginTop: 5, marginBottom: 5 }}\n              onClick={() => onSubmit({ menuNumber, image: null })}\n            >\n              Delete Image\n            </Button>\n          </div>\n        ) : id ? <ImageUpload id={id} uploadImage={uploadImage} /> : null }\n      </Paper>\n    </div>\n  );\n}\n\nMenuItemForm.propTypes = {\n  description: PropTypes.string,\n  id: PropTypes.string,\n  image: PropTypes.bool,\n  menuNumber: PropTypes.number,\n  onSubmit: PropTypes.func.isRequired,\n  price: PropTypes.number,\n  title: PropTypes.string,\n  uploadImage: PropTypes.func,\n};\n\nMenuItemForm.defaultProps = {\n  description: '',\n  id: null,\n  image: false,\n  menuNumber: null,\n  price: null,\n  title: null,\n  uploadImage: null,\n};\n"]}]}