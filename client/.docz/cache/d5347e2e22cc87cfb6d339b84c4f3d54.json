Q,CAACgB,MAAT,CAAgBC,UANJ;;AAOnB;AACAf,EAAAA,KAAK,EAAEF,QAAQ,CAACgB,MAAT,CAAgBC,UARJ;;AASnB;;AAEAX,EAAAA,QAAQ,EAAEN,QAAQ,CAACkB,IAXA;;AAYnB;;AAEAX,EAAAA,QAAQ,EAAEP,QAAQ,CAACkB;AAdA,CAArB;AAiBAjB,QAAQ,CAACkB,YAAT,GAAwB;AACtBb,EAAAA,QAAQ,EAAE,IADY;AAEtBC,EAAAA,QAAQ,EAAE;AAFY,CAAxB;;;;;;;;;;0BAzFwBN,Q","sourcesContent":["import React from 'react';\n\nimport Grid from '@material-ui/core/Grid';\nimport Typography from '@material-ui/core/Typography';\nimport Paper from '@material-ui/core/Paper';\nimport Button from '@material-ui/core/Button';\n\nimport PropType from 'prop-types';\n\nexport default function MenuItem({\n  title,\n  description,\n  number,\n  price,\n  onSelect,\n  onDelete,\n  _id,\n}) {\n  console.log(onDelete)\n  return (\n    <Paper\n      onClick={() => onSelect({\n        title,\n        description,\n        menuNumber: number,\n        price,\n        _id,\n      })}\n    >\n      <Grid container spacing={3}>\n        <Grid item xs={4}>\n          <Grid item xs={12}>\n            <Paper>\n              <Typography variant=\"h6\" gutterBottom>\n                {title}\n              </Typography>\n            </Paper>\n          </Grid>\n          <Grid item xs={12}>\n            <Paper>\n              <Typography variant=\"h6\" gutterBottom>\n                {number}\n              </Typography>\n            </Paper>\n          </Grid>\n          <Grid item xs={12}>\n            <Paper>\n              <Typography variant=\"h6\" gutterBottom>\n                $\n                {price}\n              </Typography>\n            </Paper>\n          </Grid>\n          { onDelete ? (\n            <Grid item xs={12}>\n              <Button\n                variant=\"contained\"\n                color=\"secondary\"\n                onClick={(e) => {\n                  e.preventDefault();\n                  e.stopPropagation();\n                  onDelete(number);\n                }}\n              >\n              Delete\n              </Button>\n            </Grid>\n          ) : null }\n        </Grid>\n        <Grid item xs={8}>\n          <Paper>\n            <Typography variant=\"h6\" gutterBottom>\n              {description}\n            </Typography>\n          </Paper>\n        </Grid>\n      </Grid>\n    </Paper>\n  );\n}\n\nMenuItem.propTypes = {\n  /** Description of the menu item. */\n  description: PropType.string.isRequired,\n  /** Number on the menu */\n  number: PropType.string.isRequired,\n  /** Price of the item */\n  price: PropType.string.isRequired,\n  /** Name of the menu item */\n  title: PropType.string.isRequired,\n  /** Function to be called when the menu item\n   * is clicked on. (Anywhere but the delete button) */\n  onSelect: PropType.func,\n  /** Function to be called when the delete button\n   * is clicked. */\n  onDelete: PropType.func,\n};\n\nMenuItem.defaultProps = {\n  onSelect: null,\n  onDelete: null,\n};\n"]}]}