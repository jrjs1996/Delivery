://localhost:9000/menuitems/${putData.menuNumber}`, putData, setAuthHeader());\n    dispatch({\n      type: UPDATE_MENU_ITEM,\n      payload: res.data,\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n\nexport const deleteMenuItem = menuNumber => async (dispatch) => {\n  try {\n    await axios.delete(`http://localhost:9000/menuItems/${menuNumber}`, setAuthHeader());\n    dispatch({\n      type: DELETE_MENU_ITEM,\n      payload: menuNumber,\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n"]}]}